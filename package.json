    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "event-source-polyfill": "^1.0.5",
    "express": "^4.17.3",
    "express-jwt": "^5.3.3",
    "express-session": "^1.17.1",
    "file-loader": "^3.0.1",
    "glob": "^7.1.4",
