    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "@types/jest": "^24.0.13",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "autoprefixer": "^9.7.6",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
