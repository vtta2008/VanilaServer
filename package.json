    "sass-loader": "^7.1.0",
    "speed-measure-webpack-plugin": "^1.3.1",
    "style-loader": "^0.23.1",
    "stylelint": "^13.3.3",
    "stylelint-config-standard": "^20.0.0",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "url-loader": "^1.1.2",
    "webfontloader": "^1.6.28",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.7.0",
